upstream web {  
  server web:8000;
}


# portal
server {  
  location /static/ {    
        autoindex on;    
        alias /code/static/; 
  }
   location /media/ {    
        autoindex on;    
        alias /code/media/; 
  }

  location / {
        proxy_pass http://web;
    }
  listen 80;
  server_name localhost;
}